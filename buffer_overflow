#include <string.h>
#include <stdio.h>

void copy_string(char * str) {
    char buffer[64];
    strcpy(buffer, str);
    printf("Here u go :)");
    printf(buffer);
    printf("\n");
}

int main(int argc, char ** argv) {
    int x;
    if (argc > 1) {
        copy_string(argv[1]);
    }
    x = getchar();
    printf("du skrev: ");
    putchar( x );
}
